import os
path=os.walk("F:/Chinmayee/files")
filecount=0
foldercount=0
for root,subdirectories,file in path:
    print("rrot",root)
    print("subdirectories",subdirectories)
    print("file",file)
    filecount+=len(file)
    foldercount+=len(subdirectories)
print("total files:",filecount)
print("total subdirectories:",foldercount)
